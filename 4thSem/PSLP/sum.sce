
//Sum of 50 natural numbers...
clc;
clear;
disp("Saurav Rawat")
sum=0
for i=1:50
    sum =sum+i;
end
disp("Sum of first 50 natural numbers: ", sum)

//Using while loop...

disp("Using while loop...")
sum=0;
i=1;
while(i<51)
    sum =sum +i;
    i=i+1;
end
disp("Sum of first 50 natural numbers: ", sum)
